<T3DataStructure>
    <meta type="array">
        <langChildren>0</langChildren>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>Funktion</sheetTitle>
                <type>array</type>
                <el>
                    <switchableControllerActions>
                        <label>Select function</label>
                        <onChange>reload</onChange>
                        <config>
                            <type>select</type>
                            <items>
                                <numIndex index="0">
                                    <label>Map</label>
                                    <value>Map->show</value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>Places</label>
                                    <value>Place->list;Place->show;Place->ajaxShow</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>Search</label>
                                    <value>Map->search;Map->show</value>
                                </numIndex>
                            </items>
                        <renderType>selectSingle</renderType>
                    </config>
                    </switchableControllerActions>
                    <settings.map>
                        <label>Map</label>
                        <displayCond>FIELD:switchableControllerActions:=:Map->show</displayCond>
                        <config>
                            <type>group</type>
                            <allowed>tx_ajaxmap_domain_model_map</allowed>
                            <size>1</size>
                            <minitems>1</minitems>
                            <maxitems>1</maxitems>
                        </config>
                    </settings.map>
                </el>
            </ROOT>
        </sDEF>
        <sorting>
            <ROOT>
                <sheetTitle>
                        LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.sorting
                    </sheetTitle>
                <type>array</type>
                <el>
                    <!-- order by -->
                    <settings.orderBy>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.orderBy</label>
                        <displayCond>FIELD:sDEF.switchableControllerActions:!=:Map->show</displayCond>
                        <config>
                            <type>select</type>
                            <items>
                                <numIndex index="0" type="array">
                                    <label></label>
                                    <value></value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>
                                        LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.title
                                    </label>
                                    <value>title</value>
                                </numIndex>
                            </items>
                        <renderType>selectSingle</renderType>
                        </config>
                    </settings.orderBy>
                    <!-- order direction -->
                    <settings.orderDirection>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.orderDirection</label>
                        <displayCond>FIELD:sDEF.switchableControllerActions:!=:Map->show</displayCond>
                        <config>
                            <type>select</type>
                            <items>
                                <numIndex index="0" type="array">
                                    <label></label>
                                    <value></value>
                                </numIndex>
                                <numIndex index="1">
                                    <label>
                                        LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.orderDirection.ascending
                                    </label>
                                    <value>asc</value>
                                </numIndex>
                                <numIndex index="2">
                                    <label>
                                        LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.orderDirection.descending
                                    </label>
                                    <value>desc</value>
                                </numIndex>
                            </items>
                        <renderType>selectSingle</renderType>
                        </config>
                    </settings.orderDirection>
                </el>
            </ROOT>
        </sorting>
        <!-- SHEET CONSTRAINTS -->
        <constraints>
            <ROOT>
                <sheetTitle>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.constraints</sheetTitle>
                <type>array</type>
                <el>
                    <settings.locationTypes>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.locationTypes</label>
                        <displayCond>FIELD:sDEF.switchableControllerActions:=:Place->list;Place->show;Place->ajaxShow</displayCond>
                        <config>
                            <type>group</type>
                            <allowed>tx_ajaxmap_domain_model_locationtype</allowed>
                            <size>10</size>
                            <minitems>0</minitems>
                            <maxitems>9999</maxitems>
                        </config>
                    </settings.locationTypes>
                    <settings.placeGroups>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.placeGroups</label>
                        <displayCond>FIELD:sDEF.switchableControllerActions:=:Place->list;Place->show;Place->ajaxShow</displayCond>
                        <config>
                            <type>group</type>
                            <allowed>tx_ajaxmap_domain_model_placegroup</allowed>
                            <size>10</size>
                            <minitems>0</minitems>
                            <maxitems>9999</maxitems>
                        </config>
                    </settings.placeGroups>
                    <settings.map>
                        <label>Map</label>
                        <displayCond>FIELD:sDEF.switchableControllerActions:=:Place->list;Place->show;Place->ajaxShow</displayCond>
                        <config>
                            <type>group</type>
                            <allowed>tx_ajaxmap_domain_model_map</allowed>
                            <size>1</size>
                            <minitems>0</minitems>
                            <maxitems>1</maxitems>
                        </config>
                    </settings.map>
                </el>
            </ROOT>
        </constraints>
        <display>
            <ROOT>
                <sheetTitle>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.display</sheetTitle>
                <type>array</type>
                <el>
                    <settings.showRadiusSearch>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.showRadiusSearch</label>
                        <displayCond>
                            <OR>
                                <numIndex index="0">FIELD:sDEF.switchableControllerActions:=:Map->show</numIndex>
                                <numIndex index="1">FIELD:sDEF.switchableControllerActions:=:Map->search;Map->show</numIndex>
                            </OR>
                        </displayCond>
                        <config>
                            <type>check</type>
                            <default>0</default>
                        </config>
                    </settings.showRadiusSearch>
                    <settings.showSearchField>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.showSearchField</label>
                        <displayCond>
                            <OR>
                                <numIndex index="0">FIELD:sDEF.switchableControllerActions:=:Map->show</numIndex>
                                <numIndex index="1">FIELD:sDEF.switchableControllerActions:=:Map->search;Map->show</numIndex>
                            </OR>
                        </displayCond>
                        <config>
                            <type>check</type>
                            <default>1</default>
                        </config>
                    </settings.showSearchField>
                </el>
            </ROOT>
        </display>
        <!-- SHEET PAGES -->
        <pages>
            <ROOT>
                <sheetTitle>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.pages</sheetTitle>
                <type>array</type>
                <el>
                    <settings.showPid>
                        <label>LLL:EXT:ajaxmap/Resources/Private/Language/locallang_be.xlf:flexforms_general.showPage</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <minitems>0</minitems>
                            <maxitems>1</maxitems>
                            <wizards>
                                <suggest>
                                    <type>suggest</type>
                                </suggest>
                            </wizards>
                        </config>
                    </settings.showPid>
                </el>
            </ROOT>
        </pages>
    </sheets>
</T3DataStructure>
