<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <!-- typo 3 font nur zum test -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro&display=swap" rel="stylesheet">
    <!--script src="https://maps.googleapis.com/maps/api/js" type="text/javascript"></script-->
</head>
<body>


<div id="c35" class="u-space-b:default">

    <!--
        map-module
        height default 100vh
    -->

    <div class="c-ajax-map js-ajax-map u-space-b:default c-ajax-map--sb-is-open" data-height="100vh">

        <!-- inline js -->
        <script type="text/javascript">
            var mapStore = null;

            if (!mapStore) {
                mapStore = [];
            }

            if (typeof regionStore === "undefined") {
                var regionStore = [];
            }

            if (typeof categoryTreeStore === "undefined") {
                var categoryTreeStore = [];
            }

            var mapSettings = {{{ map }}};

            mapStore.push(mapSettings);
            regionStore.push({"id": mapSettings.id});
            categoryTreeStore.push({"id": mapSettings.id});

            var ajaxMapConfig = window.ajaxMapConfig;
            window.ajaxMapConfig = ajaxMapConfig ? ajaxMapConfig : {};

        </script>

        <!-- map -->
        <div class="c-ajax-map__map-wrapper">
            <div id="ajaxMapContainer_Map1" class="c-ajax-map__map"></div>
        </div>


        <!-- sidebar -->
        <div id="placesTreeWrap" class="c-ajax-map__sb">
            <button type="button" class="c-ajax-map__sb__trigger js-ajax-map-sb-trigger"
                    aria-label="open/close map sidebar">
                open/close map sidebar
            </button>

            <!-- filter -->
            <header class="c-ajax-map__sb__filter">
                {{render '@suggest-search-input'}}
                {{render '@option-filter-select'}}
                {{render '@option-filter-checkbox'}}
                {{render '@radial-search-input'}}
                {{render '@list-search-input'}}

                <!-- TODO Emi styling Treelist -->
                <div id="ajaxMapRegionsTree1" class="dynaTree"></div>
                <div id="ajaxMapLocationTypesTree1" class="dynaTree"></div>
                <div id="ajaxMapCategoryTree1" class="dynaTree"></div>
                <div id="ajaxMapPlaceGroupTree1" class="dynaTree"></div>
            </header>

            <!-- results -->
            <div class="c-ajax-map__scroll-wrapper">
                <div class="c-ajax-map__msg">
                    Platz f√ºr Hinweistexte
                </div>

                <!-- dynamic list => render js -->
                <div id="ajaxMapPlacesTree1" class="c-ajax-map-list dynaTree"></div>
            </div>
            <footer class="c-ajax-map__sb__header">
                Reset filter
            </footer>
        </div>

        <!-- ajax form hidden -->
        <form name="txAjaxmapStore"
              action="/beraterkarte-2?tx_ajaxmap_map%5Bcontroller%5D=Map&amp;cHash=7a54e36bceaa376d21d6255e28ebb40a"
              method="post">
            <div>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][@extension]" value="Ajaxmap"/>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][@vendor]" value="DWenzel"/>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][@controller]" value="Map"/>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][@action]" value="show"/>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][arguments]"
                       value="YTowOnt9f86ca4fd3e3b6aacedcfb2344bc198b4d44cfebb"/>
                <input type="hidden" name="tx_ajaxmap_map[__referrer][@request]"
                       value="a:4:{s:10:&quot;@extension&quot;;s:7:&quot;Ajaxmap&quot;;s:11:&quot;@controller&quot;;s:3:&quot;Map&quot;;s:7:&quot;@action&quot;;s:4:&quot;show&quot;;s:7:&quot;@vendor&quot;;s:7:&quot;DWenzel&quot;;}6beac5590010337ac3931c66cd706544a46cc564"/>
                <input type="hidden" name="tx_ajaxmap_map[__trustedProperties]"
                       value="a:1:{s:14:&quot;txAjaxmapStore&quot;;a:1:{s:5:&quot;mapId&quot;;i:1;}}d2f9d8e7dd4ea12d85c9bb4c7102601b9d8ac873"/>
            </div>
            <input type="hidden" name="tx_ajaxmap_map[txAjaxmapStore][mapId]" value="1"/></form>
    </div>
</div>

</body>
</html>
